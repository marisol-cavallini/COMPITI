#include <stdio.h>
#define DIM 5
int main(int argc, char*argv[])
{
    int i;
    int numero[DIM];
    for (i=0; i < DIM; i++) {
        printf ("inserisci numero:\n");
        scanf("%d",&numero[i]);
    }
    printf ("i numeri invertiti sono:\n");
    for (i=DIM-1; i < DIM & i>=0; i--)
	{
	    
		printf("%d\n",numero[i]);
	}
    
    return 0;
}
